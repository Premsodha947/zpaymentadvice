@Metadata.layer: #CORE
@UI:{ 
headerInfo:{ typeName: 'Post Outgoing Payment' }}

annotate entity ZI_Payment_Header
    with {
@UI.facet:[{ 
    id: 'identification',
    purpose: #STANDARD,
    type: #IDENTIFICATION_REFERENCE,
    label: 'Post Outgoing Payment' }]
    
    @UI: { 
    identification: [{position: 1}],
    lineItem: [{position: 1}],
    selectionField: [{ position: 1 }]}
    AccountingDocument;
    @UI: { identification: [{position: 10}], lineItem: [{position: 10}] }
CompanyCode;

@UI: { lineItem: [
              {type: #FOR_ACTION,
               dataAction: 'print',
               label: 'PRINT'}  ]}

@UI: { identification: [{position: 20}], lineItem: [{position: 20}] }
