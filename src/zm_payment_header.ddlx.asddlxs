@UI: { identification: [{position: 40}], lineItem: [{position: 40}] }
Name;

@UI: { identification: [{position: 50}], lineItem: [{position: 50}] }
City;

@UI: { identification: [{position: 60}], lineItem: [{position: 60}] }
Street;

@UI: { identification: [{position: 70}], lineItem: [{position: 70}] }
Region;

@UI: { identification: [{position: 80}], lineItem: [{position: 80}] }
Country;

@UI: { identification: [{position: 90}], lineItem: [{position: 90}] }
Supplier;

@UI: { identification: [{position: 100}], lineItem: [{position: 100}] }
Documentdate;



    
    
}